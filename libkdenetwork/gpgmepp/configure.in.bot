if test -z "$GPGME_LIBS" || test -n "$kdepim_needs_gpgme_copy" ; then
   echo
   echo "You are missing gpgme 0.4.5 or higher.  The build will fail."
   echo "Download gpgme >= 0.4.5 from ftp://ftp.gnupg.org/gcrypt/alpha/gpgme"
   echo "or use the --with-gpgme-prefix=/path/where/gpgme/is/installed option."
   echo
   all_tests=bad
fi
