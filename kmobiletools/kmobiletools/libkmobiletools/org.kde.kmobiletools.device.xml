<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.kmobiletools.device">
    <method name="isConnected">
      <arg type="b" direction="out"/>
    </method>
    <method name="encodings">
      <arg type="as" direction="out"/>
    </method>
    <method name="stopDevice"/>
    <method name="resumeDevice"/>
    <method name="switch2filesystem"/>
    <method name="deviceVendor">
      <arg type="s" direction="out"/>
    </method>
    <method name="deviceModel">
      <arg type="s" direction="out"/>
    </method>
    <method name="raiseDevice"/>
    <method name="friendlyName">
      <arg type="s" direction="out"/>
    </method>
    <method name="sendSMS">
      <arg type="s" direction="in"/>
      <arg type="s" direction="in"/>
    </method>
    <method name="storeSMS">
      <arg type="s" direction="in"/>
      <arg type="s" direction="in"/>
    </method>
    <method name="slotFetchPhonebook"/>
    <method name="slotFetchCalendar"/>
    <method name="slotExportSMSList"/>
    <method name="slotNewSMS"/>
    <method name="slotNewSMS">
      <arg type="s" direction="in"/>
    </method>
    <method name="slotDialNumber">
      <arg type="s" direction="in"/>
    </method>
    <method name="raisePage">
      <arg type="i" direction="in"/>
    </method>
    <method name="currentDeviceName">
      <arg type="s" direction="out"/>
    </method>
  </interface>
</node>
