msgid "<shortcut> <keycap>F4</keycap> </shortcut> <guimenu>Artikel</guimenu> <guimenuitem>Suchen</guimenuitem>"
msgstr "<shortcut> <keycap>F4</keycap> </shortcut> <guimenu><accel>A</accel>rticle</guimenu> <guimenuitem>Search</guimenuitem>"

