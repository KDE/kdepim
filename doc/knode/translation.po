msgid "<shortcut> <keycap>W</keycap> </shortcut> <guimenu>Artikel</guimenu> <guisubmenu>Diskussion</guisubmenu> <guimenuitem>Überwachen</guimenuitem>"
msgstr "<shortcut> <keycap>W</keycap> </shortcut> <guimenu><accel>A</accel>rticle</guimenu> <guisubmenu><accel>T</accel>hread</guisubmenu> <guimenuitem><accel>W</accel>atch</guimenuitem>"

#: index.docbook:4566
msgid "Setzt den Status der Artikel der aktiven Diskussion auf \"überwacht\"."
msgstr "<action>Set the status of the active thread</action> to <quote>watched</quote>"

