msgid "<shortcut> <keycombo><keycap>Strg</keycap><keycap>U</keycap></keycombo> </shortcut> <guimenu>Artikel</guimenu> <guisubmenu>Diskussion</guisubmenu> <guimenuitem>Diskussion als ungelesen markieren </guimenuitem>"
msgstr "<shortcut> <keycombo action=\"simul\"><keycap>Ctrl</keycap><keycap>U</keycap></keycombo> </shortcut> <guimenu><accel>A</accel>rticle</guimenu> <guisubmenu><accel>T</accel>hread</guisubmenu> <guimenuitem>Mark thread as u<accel>n</accel>read </guimenuitem>"

#: index.docbook:4566
msgid "Setzt den Status der Artikel der aktiven Diskussion auf \"ungelesen\"."
msgstr "<action>Set the status of the active thread</action> to <quote>unread</quote>"

#: index.docbook:4566
msgid "<shortcut> <keycap>S</keycap> </shortcut> <guimenu>Artikel</guimenu> <guisubmenu>Diskussion</guisubmenu> <guimenuitem>Bewertung vergeben...</guimenuitem>"
