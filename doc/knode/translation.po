msgstr "<action>Sends the active article.</action> You can only use this in the folders <guilabel>Outbox</guilabel> and <guilabel>Drafts</guilabel>."

#: index.docbook:4566
msgid "<guimenu>Artikel</guimenu> <guimenuitem>Später versenden</guimenuitem>"
msgstr "<guimenu><accel>A</accel>rticle</guimenu> <guimenuitem>Send Later</guimenuitem>"

