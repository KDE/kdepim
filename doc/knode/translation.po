msgid "Setzt den Status der Artikel der aktiven Diskussion auf \"ignorieren\"."
msgstr "<action>Set the status of the active thread</action> to <quote>ignored</quote>"

#: index.docbook:4566
msgid "<guimenu>Artikel</guimenu> <guimenuitem>Zurücknehmen </guimenuitem>"
msgstr "<guimenu><accel>A</accel>rticle</guimenu> <guimenuitem><accel>C</accel>ancel </guimenuitem>"

