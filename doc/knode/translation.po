msgid "<guimenu>Gruppe</guimenu> <guimenuitem>Eigenschaften</guimenuitem>"
msgstr "<guimenu>G<accel>r</accel>oup</guimenu> <guimenuitem><accel>P</accel>roperties</guimenuitem>"

