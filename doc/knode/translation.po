msgid "<shortcut> <keycap>I</keycap> </shortcut> <guimenu>Artikel</guimenu> <guisubmenu>Diskussion</guisubmenu> <guimenuitem>Ignorieren</guimenuitem>"
msgstr "<shortcut> <keycap>I</keycap> </shortcut> <guimenu><accel>A</accel>rticle</guimenu> <guisubmenu><accel>T</accel>hread</guisubmenu> <guimenuitem><accel>I</accel>gnore</guimenuitem>"

