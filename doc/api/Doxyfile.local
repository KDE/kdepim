## Warn about everything in PIM, too, just like in kdelibs.
WARN_IF_UNDOCUMENTED = YES
## In PIM we're even stricter and want all parameters
## to be documented as well.
WARN_NO_PARAMDOC = YES

## Sort methods to make 'same as above' easier.
SORT_MEMBER_DOCS = YES

## Remove unsightly export macros
PREDEFINED = KDECORE_EXPORT="" \
	KDEUI_EXPORT="" \
	KDE_EXPORT="" \
	Q_SLOTS="slots" \
	Q_SIGNALS="signals"
