<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui name="headerthemeeditor" version="1">
 <MenuBar>
  <Menu name="file" >
   <Action name="new_theme" />
   <Separator />
   <Action name="insert_file" />
   <Separator />
   <Action name="upload_theme"/>
  </Menu>
  <Menu noMerge="1" name="edit" >
   <text>&amp;Edit</text>
   <Action name="add_extra_page" />
   <Separator />
   <Action name="install_theme" />
  </Menu>
  <Menu name="Display" >
   <text>&amp;Display</text>
   <Action name="normal_mode" />
   <Action name="printing_mode" />
  </Menu>
 </MenuBar>
</kpartgui>

