	-rm -f  kmailcvt.moc.cpp

#>+ 2
docs-am:

#>+ 5
force-reedit:
		cd $(top_srcdir) && $(AUTOMAKE) --gnu kmailcvt/Makefile
	cd $(top_srcdir) && perl admin/am_edit kmailcvt/Makefile.in


#>+ 10
kmailcvt_meta_unload.cpp:  kmailcvt.moc.cpp
	@echo 'creating kmailcvt_meta_unload.cpp'
	-rm -f kmailcvt_meta_unload.cpp
	@echo 'static const char * _metalist_kmailcvt[] = {' > kmailcvt_meta_unload.cpp
	cat  kmailcvt.moc.cpp | grep 'char.*className' | sed -e 's/.*[^A-Za-z0-9_:]\([A-Za-z0-9_:]*\)::className.*$$/\"\1\",/' | sort | uniq >> kmailcvt_meta_unload.cpp
	@echo '0};' >> kmailcvt_meta_unload.cpp
	@echo '#include <kunload.h>' >> kmailcvt_meta_unload.cpp
	@echo '_UNLOAD(kmailcvt)' >> kmailcvt_meta_unload.cpp


#>+ 3
clean-moc-classes:
	-rm -f  kmailcvt_meta_unload.cpp

#>+ 11
kmailcvt.all_cxx.cxx: $(srcdir)/Makefile.in $(srcdir)/liboe.cxx $(srcdir)/filter_oe4.cxx $(srcdir)/pablib.cxx $(srcdir)/filters.cxx $(srcdir)/filter_pab.cxx $(srcdir)/filter_oe5.cxx  
	@echo 'creating kmailcvt.all_cxx.cxx ...'; \
	rm -f kmailcvt.all_cxx.files kmailcvt.all_cxx.final; \
	echo "#define KDE_USE_FINAL 1" >> kmailcvt.all_cxx.final; \
	for file in liboe.cxx filter_oe4.cxx pablib.cxx filters.cxx filter_pab.cxx filter_oe5.cxx ; do \
	  echo "#include \"$$file\"" >> kmailcvt.all_cxx.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> kmailcvt.all_cxx.final; \
	done; \
	cat kmailcvt.all_cxx.final kmailcvt.all_cxx.files  > kmailcvt.all_cxx.cxx; \
	rm -f kmailcvt.all_cxx.final kmailcvt.all_cxx.files

#>+ 11
kmailcvt.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/oe4_2mbox.cpp $(srcdir)/oe5_2mbox.cpp $(srcdir)/mapihd.cpp $(srcdir)/kmailcvt.cpp $(srcdir)/main.cpp kmailcvt_meta_unload.cpp  kmailcvt.moc.cpp
	@echo 'creating kmailcvt.all_cpp.cpp ...'; \
	rm -f kmailcvt.all_cpp.files kmailcvt.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> kmailcvt.all_cpp.final; \
	for file in oe4_2mbox.cpp oe5_2mbox.cpp mapihd.cpp kmailcvt.cpp main.cpp kmailcvt_meta_unload.cpp  kmailcvt.moc.cpp; do \
	  echo "#include \"$$file\"" >> kmailcvt.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> kmailcvt.all_cpp.final; \
	done; \
	cat kmailcvt.all_cpp.final kmailcvt.all_cpp.files  > kmailcvt.all_cpp.cpp; \
	rm -f kmailcvt.all_cpp.final kmailcvt.all_cpp.files

#>+ 3
