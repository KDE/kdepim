<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.kpilot.daemon">
    <method name="requestSync">
	<annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
	<arg type="i" direction="in"/>
    </method>
    <method name="requestSyncType">
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
        <arg type="s" direction="in"/>
    </method>
    <method name="requestRegularSyncNext">
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="nextSyncType">
	<arg type="i" direction="out"/>
    </method>
    <method name="requestSyncOptions">
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
	<arg name="test" type="b" direction="in"/>
        <arg name="local" type="b" direction="in"/>
    </method>
    <method name="quitNow">
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="reloadSettings">
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
    </method>
    <method name="setTempDevice">
        <annotation name="org.freedesktop.DBus.Method.NoReply" value="true"/>
	<arg type="s" direction="in"/>
    </method>
    <method name="stopListening"/>
    <method name="startListening"/>
    <method name="isListening">
	<arg type="b" direction="out"/>
    </method>
    <method name="statusString">
        <arg type="s" direction="out"/>
    </method>
    <method name="shortStatusString">
        <arg type="s" direction="out"/>
    </method>
    <method name="configuredConduitList">
        <arg type="as" direction="out"/>
    </method>
    <method name="logFileName">
        <arg type="s" direction="out"/>
    </method>
    <method name="userName">
        <arg type="s" direction="out"/>
    </method>
    <method name="pilotDevice">
        <arg type="s" direction="out"/>
    </method>
    <method name="killDaemonOnExit">
        <arg type="b" direction="out"/>
    </method>
    <!-- <method name="lastSyncDate">
        <arg type="(iii)" direction="out"/>
      <annotation name="com.trolltech.QtDBus.QtTypeName.Out0" value="QDateTime"/>
    </method> -->
    <signal name="kpilotDaemonStatusChanged"/>
    <signal name="kpilotDaemonStatusDetails">
      <arg type="ay" direction="in"/>
      <arg type="(iii)" direction="in"/>
      <annotation name="com.trolltech.QtDBus.QtTypeName.In0" value="QDateTime"/>
      <arg type="s" direction="in"/>
      <arg type="as" direction="in"/>
      <arg type="s" direction="in"/>
      <arg type="s" direction="in"/>
      <arg type="s" direction="in"/>
      <arg type="b" direction="in"/>
    </signal>
  </interface>
</node>
