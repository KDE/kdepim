                    <property name="name">
                        <cstring>fPopUser</cstring>
                    </property>
                    <property name="text">
                        <string>$USER</string>
                    </property>
                </widget>
                <widget class="KLineEdit" row="1" column="1" rowspan="2" colspan="1">
                    <property name="name">
                        <cstring>fPopServer</cstring>
