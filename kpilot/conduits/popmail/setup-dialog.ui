                    </property>
                </widget>
                <widget class="QLabel" row="4" column="0">
                    <property name="name">
                        <cstring>textLabel3</cstring>
                    </property>
                    <property name="text">
                        <string>Sendmail:</string>
                    </property>
                </widget>
                <widget class="KLineEdit" row="4" column="1">
                    <property name="name">
                        <cstring>fSendmailCmd</cstring>
                    </property>
                    <property name="text">
                        <string>/usr/lib/sendmail -t -i</string>
                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Enter the command used to execute sendmail on your system.</string>
                    </property>
                </widget>
                <widget class="QComboBox" row="0" column="1">
