                    <property name="text">
                        <string>POP server:</string>
                    </property>
                </widget>
                <widget class="QLabel" row="3" column="0">
                    <property name="name">
                        <cstring>textLabel9</cstring>
                    </property>
                    <property name="text">
                        <string>POP username:</string>
                    </property>
                </widget>
                <widget class="QLabel" row="4" column="0">
