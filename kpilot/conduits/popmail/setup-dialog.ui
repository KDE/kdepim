                    <property name="text">
                        <string>pop:110</string>
                    </property>
                </widget>
                <widget class="QComboBox" row="0" column="1">
                    <item>
                        <property name="text">
                            <string>Do Not Receive Mail</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Use POP3 Server</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Use System Mailbox</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>fRecvMode</cstring>
                    </property>
                </widget>
                <widget class="KURLRequester" row="6" column="1">
                    <property name="name">
                        <cstring>fMailbox</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="6" column="0">
                    <property name="name">
                        <cstring>textLabel6</cstring>
                    </property>
                    <property name="text">
                        <string>System mailbox:</string>
                    </property>
                </widget>
                <widget class="QCheckBox" row="5" column="0">
                    <property name="name">
                        <cstring>fLeaveMail</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Leave mail on server</string>
                    </property>
                </widget>
                <widget class="QCheckBox" row="5" column="1">
                    <property name="name">
                        <cstring>fStorePass</cstring>
                    </property>
                    <property name="text">
                        <string>Save &amp;POP password</string>
                    </property>
                </widget>
            </grid>
        </widget>
